{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": { 
        "linebreak-style": "warn",
        "import/prefer-default-export": "warn",
        "no-self-assign": "warn"
    }
}